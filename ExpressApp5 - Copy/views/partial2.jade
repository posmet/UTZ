                th(scoup="col")	Остаток
                th(scoup="col")	ВПути
                th(scoup="col")	Заявка
        tbody
            tr(ng-repeat="row in Result")
                td {{row.GrCode}}
                td {{row.Gr_Name}}
                td {{row.Ph_ID}}
                td {{row.Ph_Name}}
                td {{row.Order}}
                td {{row.DateRq}}
                td {{row.Ratio}}
                td {{row.MinQty}}
                td {{row.MinReq}}
                td {{row.Status}}
                td {{row.PriceIn}}
                td {{row.PriceOut}}
