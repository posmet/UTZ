div(ng-controller="MyCtrl3")
    p Настройка параметров обработки активной матрицы.
    {{login}}
    {{rq}}

    p Обработка заявки.
    table(class="table table-bordered table-striped")
        thead(class="thead-light")
            tr
                th(scoup="col")	ГрКод
                th(scoup="col")	Наименование
                th(scoup="col")	КодСклада
                th(scoup="col")	Аптека
                th(scoup="col")	НомерЗаказа
                th(scoup="col")	ДатаЗаказа
                th(scoup="col")	Кратность
